{
  "name": "piercing-library",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./index.d.ts",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "npm run build.host-inline-script; npm run build.lib",
    "build.host-inline-script": "tsc && vite build -c ./piercing-fragment-host.vite.config.ts",
    "build.lib": "tsc && vite build",
    "prebuild.watch": "npm run prebuild",
    "build.watch": "concurrently 'npm run build.host-inline-script.watch' 'npm run build.lib.watch'",
    "build.host-inline-script.watch": "tsc && vite build -c ./piercing-fragment-host.vite.config.ts -w",
    "build.lib.watch": "tsc && vite build -w"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "writable-dom": "^1.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@types/node": "^18.7.15",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "concurrently": "^7.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.4.1"
  }
}
