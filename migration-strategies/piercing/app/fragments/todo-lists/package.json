{
	"name": "productivity-suite-todo-lists-fragment",
	"description": "Todo Lists Fragment Implemented using Qwik",
	"scripts": {
		"build": "npm run build.client && npm run build.ssr",
		"build-local": "npm run build.client-local && npm run build.ssr-local",
		"build.client": "vite build",
		"build.client-local": "vite build --mode development-local",
		"build.ssr": "vite build --ssr src/entry.ssr.tsx",
		"build.ssr-local": "vite build --mode development-local --ssr src/entry.ssr.tsx",
		"lint": "eslint \"src/**/*.ts*\"",
		"start": "npx wrangler dev --local --env local",
		"dev": "npm run start",
		"deploy": "npx wrangler publish",
		"typecheck": "tsc --incremental --noEmit"
	},
	"dependencies": {
		"cookie": "^0.5.0",
		"piercing-library": "file:../../../piercing-library"
	},
	"devDependencies": {
		"@builder.io/qwik": "0.10.0",
		"@cloudflare/workers-types": "^3.14.1",
		"@types/cookie": "^0.5.1",
		"@types/eslint": "8.4.5",
		"@types/express": "4.17.13",
		"@types/node": "latest",
		"@types/react": "^18.0.5",
		"@types/react-dom": "^18.0.1",
		"@typescript-eslint/eslint-plugin": "5.30.6",
		"@typescript-eslint/parser": "5.30.6",
		"eslint": "8.20.0",
		"eslint-plugin-qwik": "0.0.113",
		"express": "4.17.3",
		"node-fetch": "3.2.8",
		"prettier": "2.7.1",
		"typescript": "4.7.4",
		"vite": "3.0.1",
		"vite-tsconfig-paths": "3.5.0",
		"wrangler": "2.1.10"
	},
	"engines": {
		"node": ">=14"
	},
	"license": "",
	"private": true,
	"type": "module"
}
